<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd ">
<struts>
    <!-- 定义逻辑视图与物理视图之间的联系 -->
    <package name="trio" extends="struts-default">
    
     <!-- 返回登录界面  -->
        <action name="home" class="com.structs.trio.Jump" method="returnHome">
            <result name="SUCCESS">/index.html</result>
        </action>
        
     <!-- 成功  -->
        <action name="success" class="com.structs.trio.Assign_in" method="add_information">
            <result name="SUCCESS">/index.html</result>
        </action>
        
     <!-- 登录  -->
        <action name="login" class="com.structs.trio.Login_in" method="login">
            <result name="SUCCESS">/showinfo.jsp</result>
        </action>
     
      
    </package>
</struts>


